{
  "name": "ng-datable-demo",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Minds/ng-data-table.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test -sm=false",
    "lint": "ng lint",
    "update": "ng update",
    "e2e": "ng e2e",
    "build:docs": "compodoc ./libs -p src/tsconfig.packages.json -s",
    "build:app": "cpr dist/ng-data-table src/ng-data-table --overwrite && ng build app packages && rimraf src/ng-data-table",
    "build:app:prod": "cpr dist/ng-data-table src/ng-data-table --overwrite && ng build --prod --named-chunks=true app packages && rimraf src/ng-data-table",
    "build:libs:datatable": "ng-packagr -p libs/datatable/package.json",
    "publish:libs:datatable": "npm publish dist/ng-data-table"
  },
  "dependencies": {
    "@angular/animations": "~6.0.0",
    "@angular/cli": "^6.0.1",
    "@angular/common": "~6.0.0",
    "@angular/compiler": "~6.0.0",
    "@angular/core": "~6.0.0",
    "@angular/forms": "~6.0.0",
    "@angular/http": "~6.0.0",
    "@angular/platform-browser": "~6.0.0",
    "@angular/platform-browser-dynamic": "~6.0.0",
    "@angular/router": "~6.0.0",
    "core-js": "~2.5.3",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "lodash-es": "^4.17.4",
    "popper.js": "^1.14.3",
    "rxjs": "~6.1.0",
    "tslint-angular": "^1.1.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.3.2",
    "@angular-devkit/schematics": "0.3.2",
    "@angular/cli": "^6.0.1",
    "@angular/compiler-cli": "~6.0.0",
    "@angular/language-service": "~6.0.0",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.47",
    "codelyzer": "^4.2.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.4",
    "node-sass": "4.7.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "ts-node": "~6.0.3",
    "tsickle": "^0.26.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.1"
  }
}
